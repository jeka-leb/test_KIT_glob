{
  "extends": ["react-app", "react-app/jest"],
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "warn",
    "quotes": ["warn", "single"],
    "jsx-quotes": ["warn", "prefer-double"],
    "prefer-const": ["error"],
    "indent": ["warn", 2],
    "max-len": [
      "error",
      {
        "code": 120
      }
    ],
    "comma-dangle": ["error", "always-multiline"],
    "semi": ["warn", "always"],
    "import/order": [
      "error",
      {
        "newlines-between": "always-and-inside-groups"
      }
    ]
  }
}
